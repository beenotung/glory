{
	"name": "camouflage",
	"version": "0.1.0",
	"author": "Hugo Sum",
	"license": "MIT",
	"type": "commonjs",
	"scripts": {
		"build": "echo 'building'",
		"test": "npx jest",
		"test:coverage": "npx jest --coverage",
		"test:watch": "npx jest --watch",
		"prepublish": "yarn build"
	},
	"exports": {
		".": {
			"require": "./index.js",
			"default": "./index.js"
		},
		"./jsx": {
			"require": "./plugins/jsx.js",
			"default": "./plugins/jsx.js"
		},
		"./hydration": {
			"require": "./plugins/hydration.js",
			"default": "./plugins/hydration.js"
		},
		"./prefixer": {
			"require": "./plugins/prefixer.js",
			"default": "./plugins/prefixer.js"
		}
	},
	"dependencies": {
		"@blackblock/joli-string": "^0.1.1",
		"browser-or-node": "^1.3.0",
		"inline-style-prefixer": "^6.0.0",
		"rambda": "^6.5.1"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"esbuild": "^0.8.29",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-chai-expect": "^2.2.0",
		"eslint-plugin-chai-friendly": "^0.6.0",
		"eslint-plugin-const-immutable": "^2.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-mocha": "^8.0.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^5.0.0",
		"husky": "^4.3.7",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.3",
		"nyc": "^15.1.0",
		"ramda": "^0.27.1"
	},
	"lint-staged": {
		"**/*.js": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx lint-staged",
			"pre-push": "yarn test"
		}
	}
}
