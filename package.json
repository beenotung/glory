{
	"name": "camouflage",
	"version": "0.1.0",
	"author": "Hugo Sum",
	"license": "MIT",
	"type": "commonjs",
	"scripts": {
		"build": "npx rollup -c",
		"test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
		"test:coverage": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
		"test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
		"prepublish": "yarn build",
		"benchmark:reduce": "node benchmarks/keyframes.spec.js"
	},
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		},
		"./jsx": {
			"require": "./dist/cjs/jsx/index.js",
			"default": "./dist/esm/jsx/index.js"
		},
		"./rule": {
			"require": "./dist/cjs/rule/index.js",
			"default": "./dist/esm/rule/index.js"
		},
		"./prefixer": {
			"require": "./dist/cjs/prefixer/index.js",
			"default": "./dist/esm/prefixer/index.js"
		},
		"./hydration": {
			"require": "./dist/cjs/hydration/index.js",
			"default": "./dist/esm/hydration/index.js"
		}
	},
	"dependencies": {
		"@blackblock/joli-string": "^0.1.1",
		"browser-or-node": "^1.3.0",
		"css-in-js-utils": "^3.1.0",
		"inline-style-prefixer": "^6.0.0",
		"rambda": "^6.5.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-modules-commonjs": "^7.12.1",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"babel-jest": "^26.6.3",
		"benchmark": "^2.1.4",
		"chai": "^4.2.0",
		"esbuild": "^0.8.29",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-chai-expect": "^2.2.0",
		"eslint-plugin-chai-friendly": "^0.6.0",
		"eslint-plugin-const-immutable": "^2.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-mocha": "^8.0.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^5.0.0",
		"fast-stringify": "^2.0.0",
		"husky": "^4.3.7",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.3",
		"nyc": "^15.1.0",
		"ramda": "^0.27.1",
		"rollup": "^2.36.1",
		"rollup-plugin-multi-input": "^1.2.0"
	},
	"lint-staged": {
		"**/*.js": [
			"npx prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx lint-staged",
			"pre-push": "yarn test"
		}
	}
}
